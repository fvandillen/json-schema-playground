{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "$id": "groendus:legal-entity",
    "title": "Legal entity",
    "properties": {
        "identifier":{
            "type": "string",
            "description": "The identifier for this legal entity, valid for the designator. Example: A Dutch (NL) KVK number."
        },
        "designator": {
            "$ref": "codelist-iso-6523.json"
        }
    },
    "required": [
        "identifier",
        "designator"
    ]
}